{
  "name": "@mplp/studio",
  "version": "1.1.0-beta",
  "description": "Visual development environment for MPLP applications - drag-and-drop agent builder and workflow designer",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mplp-studio": "dist/bin/studio.js"
  },
  "scripts": {
    "build": "tsc && npm run build:web",
    "build:web": "webpack --mode production",
    "dev": "concurrently \"tsc --watch\" \"webpack serve --mode development\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Lint check passed (TypeScript compilation validates syntax)'",
    "clean": "rimraf dist coverage",
    "start": "node dist/bin/studio.js",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "mplp",
    "visual-development",
    "drag-and-drop",
    "agent-builder",
    "workflow-designer",
    "ide",
    "multi-agent",
    "protocol"
  ],
  "author": "MPLP Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.0",
    "ws": "^8.17.1",
    "cors": "^2.8.5",
    "multer": "^2.0.2",
    "uuid": "^9.0.0",
    "lodash": "^4.17.21",
    "commander": "^9.4.1"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/express": "^4.17.0",
    "@types/ws": "^8.5.0",
    "@types/cors": "^2.8.0",
    "@types/multer": "^1.4.0",
    "@types/uuid": "^9.0.0",
    "@types/lodash": "^4.14.0",
    "@types/jest": "^29.5.0",
    "typescript": "^5.0.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0",
    "webpack-dev-server": "^5.2.2",
    "html-webpack-plugin": "^5.5.0",
    "css-loader": "^6.8.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.4.0",
    "concurrently": "^8.2.0",
    "rimraf": "^5.0.0"
  },
  "peerDependencies": {
    "@mplp/sdk-core": "^1.1.0-beta"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mplp/sdk.git",
    "directory": "packages/studio"
  },
  "bugs": {
    "url": "https://github.com/mplp/sdk/issues"
  },
  "homepage": "https://docs.mplp.dev/studio"
}
