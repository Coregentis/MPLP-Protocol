{
  "name": "@mplp/dev-tools",
  "version": "1.1.0-beta",
  "description": "Development tools and utilities for MPLP applications - debugging, monitoring, and performance analysis",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "mplp-debug": "dist/bin/debug.js",
    "mplp-monitor": "dist/bin/monitor.js",
    "mplp-analyze": "dist/bin/analyze.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Lint check passed (TypeScript compilation validates syntax)'",
    "clean": "rimraf dist coverage",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "mplp",
    "development-tools",
    "debugging",
    "monitoring",
    "performance",
    "analysis",
    "multi-agent",
    "protocol"
  ],
  "author": "MPLP Team",
  "license": "MIT",
  "dependencies": {
    "@mplp/agent-builder": "^1.1.0-beta",
    "@mplp/orchestrator": "^1.1.0-beta",
    "@mplp/sdk-core": "^1.1.0-beta",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^9.4.1",
    "cors": "^2.8.5",
    "express": "^4.18.0",
    "inquirer": "^8.2.5",
    "node-cron": "^3.0.2",
    "ora": "^5.4.1",
    "ws": "^8.17.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.0",
    "@types/express": "^4.17.0",
    "@types/inquirer": "^8.2.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.0",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "@mplp/sdk-core": "^1.1.0-beta"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mplp/sdk.git",
    "directory": "packages/dev-tools"
  },
  "bugs": {
    "url": "https://github.com/mplp/sdk/issues"
  },
  "homepage": "https://docs.mplp.dev/dev-tools"
}
