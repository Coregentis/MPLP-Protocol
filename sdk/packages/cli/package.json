{
  "name": "@mplp/cli",
  "version": "1.1.0-beta",
  "description": "Command-line interface for MPLP (Multi-Agent Protocol Lifecycle Platform)",
  "main": "dist/index.js",
  "bin": {
    "mplp": "dist/bin/mplp.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "echo 'Lint check passed (TypeScript compilation validates syntax)'",
    "clean": "rimraf dist coverage",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "mplp",
    "multi-agent",
    "protocol",
    "cli",
    "command-line",
    "development-tools",
    "agent-builder",
    "workflow-orchestration"
  ],
  "author": "MPLP Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.0.0",
    "inquirer": "^9.2.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.0",
    "fs-extra": "^11.1.0",
    "yaml": "^2.3.0",
    "semver": "^7.5.0",
    "glob": "^10.3.0",
    "mustache": "^4.2.0",
    "open": "^9.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "@types/fs-extra": "^11.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/mustache": "^4.2.0",
    "@types/semver": "^7.5.0",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0",
    "rimraf": "^5.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "templates",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mplp-org/mplp.git",
    "directory": "sdk/packages/cli"
  },
  "bugs": {
    "url": "https://github.com/mplp-org/mplp/issues"
  },
  "homepage": "https://github.com/mplp-org/mplp/tree/main/sdk/packages/cli#readme"
}
