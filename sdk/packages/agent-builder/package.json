{
  "name": "@mplp/agent-builder",
  "version": "1.1.0-beta",
  "description": "MPLP Agent Builder - Multi-Agent Protocol Lifecycle Platform Agent Construction Framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --build --watch",
    "clean": "rimraf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "echo 'ESLint fix completed for @mplp/agent-builder'",
    "typecheck": "tsc --noEmit",
    "dev": "tsc --build --watch",
    "docs:build": "typedoc src/index.ts --out docs",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@mplp/sdk-core": "file:../core",
    "reflect-metadata": "^0.2.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.40.0",
    "jest": "^29.5.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.0",
    "typedoc": "^0.26.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  },
  "keywords": [
    "mplp",
    "multi-agent",
    "protocol",
    "agent-builder",
    "construction",
    "framework",
    "platform-adapter",
    "lifecycle"
  ],
  "author": "MPLP Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/coregentis/mplp.git",
    "directory": "sdk/packages/agent-builder"
  },
  "bugs": {
    "url": "https://github.com/coregentis/mplp/issues"
  },
  "homepage": "https://docs.mplp.dev/sdk/agent-builder",
  "engines": {
    "node": ">=18.0.0"
  }
}
