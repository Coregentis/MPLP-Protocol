# MPLP CLI Usage Example Environment Configuration

# Development settings
NODE_ENV=development
LOG_LEVEL=info

# CLI Configuration
CLI_OUTPUT_DIR=./output
CLI_TEMPLATE_DIR=./templates
CLI_CACHE_DIR=./.cli-cache

# Demo settings
DEMO_PROJECT_PREFIX=mplp-demo
DEMO_CLEANUP_ENABLED=true
DEMO_VERBOSE=true

# Example project settings
EXAMPLE_AUTHOR_NAME=MPLP Developer
EXAMPLE_AUTHOR_EMAIL=developer@mplp.dev
EXAMPLE_LICENSE=MIT
EXAMPLE_DESCRIPTION=Generated by MPLP CLI

# Template settings
TEMPLATE_BASIC_ENABLED=true
TEMPLATE_ADVANCED_ENABLED=true
TEMPLATE_ENTERPRISE_ENABLED=true

# Performance settings
CLI_TIMEOUT=30000
CLI_RETRY_COUNT=3
CLI_PARALLEL_LIMIT=5
