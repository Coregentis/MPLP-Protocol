# AI Coordination Example Environment Configuration

# AI Service API Keys (Optional - for real AI integration)
OPENAI_API_KEY=your_openai_api_key_here
CLAUDE_API_KEY=your_claude_api_key_here
GEMINI_API_KEY=your_gemini_api_key_here

# AI Configuration
AI_PROVIDER=mock
AI_MODEL_SELECTION=auto
AI_TEMPERATURE=0.7
AI_MAX_TOKENS=2000

# Coordination Settings
COORDINATION_TIMEOUT=30000
CONSENSUS_THRESHOLD=0.8
CONFLICT_RESOLUTION_STRATEGY=voting
DECISION_ENGINE_MODE=consensus

# Memory Configuration
SHARED_MEMORY_PERSISTENCE=true
MEMORY_SYNC_INTERVAL=1000
MEMORY_CONFLICT_RESOLUTION=latest_wins

# Communication Settings
MESSAGE_BUS_TYPE=in_memory
EVENT_DISPATCHER_ASYNC=true
PROTOCOL_HANDLER_TIMEOUT=5000

# Performance Settings
CACHE_ENABLED=true
CACHE_TTL=300000
PERFORMANCE_MONITORING=true
METRICS_COLLECTION=true

# Logging Configuration
LOG_LEVEL=info
LOG_FORMAT=json
LOG_FILE=coordination.log

# Development Settings
NODE_ENV=development
DEBUG_MODE=false
VERBOSE_LOGGING=false
